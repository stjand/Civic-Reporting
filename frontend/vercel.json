{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "installCommand": "npm install --prefix frontend",
        "buildCommand": "npm run build --prefix frontend",
        "outputDirectory": "dist"
      }
    }
  ]
}
