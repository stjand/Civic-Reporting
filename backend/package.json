{
  "name": "civic-reporter-backend",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "nodemon -r dotenv/config src/index.js",
    "start": "node -r dotenv/config src/index.js",
    "test": "node -r dotenv/config jest",
    "migrate": "node -r dotenv/config run_migrations.js",
    "seed": "node -r dotenv/config run_migrations.js",
    "migrate:make": "node -r dotenv/config knex migrate:make --knexfile knexfile.js",
    "migrate:rollback": "node -r dotenv/config knex migrate:rollback --knexfile knexfile.js",
    "db:reset": "knex migrate:rollback --all --knexfile knexfile.js && knex migrate:latest --knexfile knexfile.js && knex seed:run --knexfile knexfile.js"
  },
  "dependencies": {
    "aws-sdk": "^2.1400.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "express-rate-limit": "^6.8.1",
    "helmet": "^7.2.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.1",
    "knex": "^3.1.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.3",
    "pg": "^8.16.3",
    "redis": "^4.6.7",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "supertest": "^6.3.4"
  },
  "main": "src/index.js",
  "keywords": [
    "civic",
    "reporting",
    "api",
    "express",
    "postgresql"
  ],
  "author": "",
  "license": "ISC",
  "description": "Backend API for Civic Reporter application"
}
